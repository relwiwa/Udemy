{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "./build",
    "sourceMap": false,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "build",
    "src/app/polyfills.ts"
  ],
  "angularCompilerOptions": {
    "skipMetdadataEmit": true
  }
}
